import java.util.*;
public class Words_found{

     public static void main(String []args)throws Exception{
        String sentence []= {"sri krishna college of engineering and technology","suriya kumar","selva go"};
        String search []= {"krishna","suriya","selva"};
for(int i=0;i<sentence.length;i++){
if (sentence[i].toLowerCase().contains(search[i].toLowerCase())) {

  System.out.println(i);

} else {

  System.out.println("not found");

}}
         
      
     }
}
