import java.util.*;
public class next_greater{

     public static void main(String []args){
         
        int x,a=0;
        Scanner scanner = new Scanner(System.in);
        int n= scanner.nextInt();
        while(n > 0)
        {
            x = n % 10;
            a = a * 10 + x;
            n = n / 10;
        }
        System.out.print(a);
     
}}
