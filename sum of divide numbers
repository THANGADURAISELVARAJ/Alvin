import java.util.*;
public class HelloWorld{

     public static void main(String []args){
         
         int n,m=0;
         Scanner sc =new Scanner(System.in);
         n=sc.nextInt();
         for(int i=1;i<n;i++){
             if(n%i==0){
              System.out.println(i);
              m=m+i;
         }}
         
        System.out.println("output is :" +m);
     }
}
